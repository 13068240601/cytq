page{
  width: 100%;
  height: 100%;
}
.search_container{
  width: 100%;
  height: 100%;
  background: url(https://api.zhangcc.top/wxtqbg/bg2.jpg) no-repeat;
  background-size: cover;
  overflow: auto;
}
.search_input{
  width: 90%;
  margin: 0 auto;
  border-radius: 5px;
  border: 1px solid #ffffff;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 10px;
  box-sizing: border-box;
  height: 40px;
  margin-top: 5px;
}
.search_input input{
  width: calc(100% - 35px);
  color: #ffffff;
}
.search_input image{
  width: 25px;
  height: 25px;
}
.data{
  width: 100%;
  padding-bottom: 10px;
  box-sizing: border-box;
}
.item{
  width: 90%;
  margin: 0 auto;
  padding: 5px;
  background-color: rgba(0,0,0,0.3);
  border-radius: 5px;
  box-sizing: border-box;  
  margin-top: 5px; 
  /* display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical; */
  color: #ffffff;
}
